package com.sunbase.service;

import java.util.List;

import com.sunbase.DTO.Admin;
import com.sunbase.DTO.UpdateUserDTO;
import com.sunbase.exception.LoginException;
import com.sunbase.model.Users;

public interface UserService{

	public Users addUser(Users user);
	public Users addAdmin(Admin admin);
	public String updateUser(UpdateUserDTO updateUserDTO) throws LoginException;
	public List<Users> getAllAdmin();
	public Users getOwnProfile();
	public String removeUser(Integer id);

}
